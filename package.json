    "eslint": "^7.29.0",
    "eslint-config-next": "11.0.1",
    "husky": "^7.0.0",
    "postcss": "^8.3.2",
    "tailwindcss": "^2.1.4",
    "typescript": "4.3.2"
  },
  "husky": {
    "hooks": {
